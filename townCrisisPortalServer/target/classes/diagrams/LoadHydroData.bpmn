    </scriptTask>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow6" sourceRef="scriptlogerror" targetRef="endevent2"></sequenceFlow>
    <callActivity id="callactivity1" name="Evaluate hydro data" calledElement="evaluatehydrodata1"></callActivity>
    <sequenceFlow id="flow7" sourceRef="callactivity1" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="retrievehydrodata1" targetRef="callactivity1"></sequenceFlow>
  </process>
