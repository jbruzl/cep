    <sequenceFlow id="flow5" sourceRef="boundarytimer1" targetRef="scriptlogerror"></sequenceFlow>
    <scriptTask id="scriptlogerror" name="Log error" scriptFormat="groovy" activiti:autoStoreVariables="false">
