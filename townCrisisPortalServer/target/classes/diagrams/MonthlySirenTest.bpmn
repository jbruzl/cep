    <startEvent id="timerstartevent1" name="Timer start">
      <timerEventDefinition>
        <timeCycle>0 50 11 ? 1/1 WED#1 *</timeCycle>
      </timerEventDefinition>
    </startEvent>
    <subProcess id="subprocess1" name="Notify about siren test">
      <startEvent id="startevent1" name="Start"></startEvent>
      <scriptTask id="scripttask2" name="Prepare mail notification" scriptFormat="groovy" activiti:autoStoreVariables="true">
        <script>source = "sender@localhost.com"
					templateKey =
					"monthlySirenTest"
					templateVariable = null
					receivers =
					["sender@localhost.com"]
					messageType =
					cz.muni.fi.bruzl.ces.entity.MessageType.NOTIFICATION
				</script>
      </scriptTask>
      <scriptTask id="scripttask3" name="Prepare SMS notification" scriptFormat="groovy" activiti:autoStoreVariables="true">
        <script>messageSMS = "CEP INFO: Za 10 min probehne test siren."
					messageTypeSMS =
					cz.muni.fi.bruzl.ces.entity.MessageType.NOTIFICATION
					receiversSMS =
					["+420728484615"]
				</script>
      </scriptTask>
      <callActivity id="notifyviamail1" name="Notify via mail" calledElement="sendEmail">
        <extensionElements>
          <activiti:in sourceExpression="${source}" target="sender"></activiti:in>
          <activiti:in sourceExpression="${receivers}" target="receivers"></activiti:in>
          <activiti:in sourceExpression="${templateKey}" target="templateKey"></activiti:in>
          <activiti:in sourceExpression="${messageType}" target="messageType"></activiti:in>
          <activiti:in sourceExpression="${templateVariable}" target="templateVariable"></activiti:in>
        </extensionElements>
      </callActivity>
      <parallelGateway id="parallelgateway1" name="Exclusive Gateway"></parallelGateway>
      <parallelGateway id="parallelgateway2" name="Exclusive Gateway"></parallelGateway>
      <endEvent id="endevent1" name="End"></endEvent>
      <sequenceFlow id="flow4" sourceRef="parallelgateway1" targetRef="scripttask2"></sequenceFlow>
      <sequenceFlow id="flow5" sourceRef="parallelgateway1" targetRef="scripttask3"></sequenceFlow>
      <sequenceFlow id="flow6" sourceRef="scripttask2" targetRef="notifyviamail1"></sequenceFlow>
      <sequenceFlow id="flow8" sourceRef="notifyviamail1" targetRef="parallelgateway2"></sequenceFlow>
      <sequenceFlow id="flow10" sourceRef="parallelgateway2" targetRef="endevent1"></sequenceFlow>
      <sequenceFlow id="flow12" sourceRef="startevent1" targetRef="parallelgateway1"></sequenceFlow>
      <callActivity id="callactivity2" name="Notify via SMS" calledElement="SendSMS">
        <extensionElements>
          <activiti:in sourceExpression="${messageSMS}" target="message"></activiti:in>
          <activiti:in sourceExpression="${receiversSMS}" target="receivers"></activiti:in>
          <activiti:in sourceExpression="${messageTypeSMS}" target="messageType"></activiti:in>
        </extensionElements>
      </callActivity>
      <sequenceFlow id="flow13" sourceRef="scripttask3" targetRef="callactivity2"></sequenceFlow>
      <sequenceFlow id="flow14" sourceRef="callactivity2" targetRef="parallelgateway2"></sequenceFlow>
    </subProcess>
    <userTask id="verifycancelation1" name="Verify cancelation">
      <extensionElements>
        <activiti:formProperty id="canceltest" name="Cancel test" default="false" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <scriptTask id="preparesirentest4" name="Prepare siren test" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>println "Preparing siren test"</script>
    </scriptTask>
    <exclusiveGateway id="testcanceled1" name="Test canceled?"></exclusiveGateway>
    <sequenceFlow id="flow18" sourceRef="verifycancelation1" targetRef="testcanceled1"></sequenceFlow>
    <endEvent id="endevent3" name="Test not canceled"></endEvent>
    <sequenceFlow id="yesflow19" name="Yes" sourceRef="testcanceled1" targetRef="terminateendevent1">
