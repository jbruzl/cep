  <process id="warningChmi" name="Warning CHMI" isExecutable="true">
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="checksettings1"></sequenceFlow>
    <scriptTask id="checksettings1" name="Check settings" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>
    		println("hello")
    	</script>
    </scriptTask>
    <sequenceFlow id="flow2" sourceRef="checksettings1" targetRef="obtainwarningreport1"></sequenceFlow>
    <serviceTask id="obtainwarningreport1" name="Obtain warning report" activiti:class="cz.muni.fi.cep.warning.chmi.tasks.ObtainWeatherReport"></serviceTask>
    <startEvent id="startevent1" name="Timer start"></startEvent>
    <boundaryEvent id="weatherreportnotobtained1" name="Weather report not obtained" attachedToRef="obtainwarningreport1">
      <errorEventDefinition errorRef="weatherReportLoadError"></errorEventDefinition>
    </boundaryEvent>
    <endEvent id="errorobtainingweatherreport2" name="Error while obtaining weather report"></endEvent>
    <sequenceFlow id="flow4" sourceRef="weatherreportnotobtained1" targetRef="errorobtainingweatherreport2"></sequenceFlow>
    <endEvent id="nothingchanged1" name="Nothing changed"></endEvent>
    <sequenceFlow id="flow6" sourceRef="obtainwarningreport1" targetRef="evaluatesweatherreport1"></sequenceFlow>
    <serviceTask id="evaluatesweatherreport1" name="Evaluate weather report" activiti:delegateExpression="${evaluateWarningReport}"></serviceTask>
    <exclusiveGateway id="exclusivegateway1" name="Warning level changed?"></exclusiveGateway>
    <sequenceFlow id="flow8" sourceRef="evaluatesweatherreport1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow9" name="No" sourceRef="exclusivegateway1" targetRef="nothingchanged1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${weatherChanged==false}]]></conditionExpression>
    </sequenceFlow>
    <scriptTask id="lowerwarninglevel1" name="Lower warning level" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>println("Level lowered")</script>
    </scriptTask>
    <callActivity id="callactivity1" name="Call activity" calledElement="Notify">
      <extensionElements>
        <activiti:in sourceExpression="${publisherCode}" target="publisherCode"></activiti:in>
        <activiti:in sourceExpression="${templateKey}" target="templateKey"></activiti:in>
        <activiti:in sourceExpression="${notificationEnabled}" target="notificationEnabled"></activiti:in>
        <activiti:in sourceExpression="${templateVariable}" target="templateVariable"></activiti:in>
        <activiti:in source="cz.muni.fi.cep.core.bpmn.service.api.MessageType.NOTIFICATION" target="messageType"></activiti:in>
      </extensionElements>
    </callActivity>
    <sequenceFlow id="flow11" sourceRef="lowerwarninglevel1" targetRef="callactivity1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Warning level risen?"></exclusiveGateway>
    <sequenceFlow id="flow12" name="Yes" sourceRef="exclusivegateway1" targetRef="exclusivegateway2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${weatherChanged==true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow13" name="No" sourceRef="exclusivegateway2" targetRef="lowerwarninglevel1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${warningLevelRisen==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" name="Yes" sourceRef="exclusivegateway2" targetRef="risewarninglevel2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${warningLevelRisen==true}]]></conditionExpression>
    </sequenceFlow>
    <callActivity id="callactivity2" name="Call activity" calledElement="Notify"></callActivity>
    <sequenceFlow id="flow15" sourceRef="risewarninglevel2" targetRef="callactivity2"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow16" sourceRef="callactivity1" targetRef="endevent1"></sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow17" sourceRef="callactivity2" targetRef="endevent2"></sequenceFlow>
    <scriptTask id="risewarninglevel2" name="Rise warning level" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>println("Level risen")</script>
    </scriptTask>
