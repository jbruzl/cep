      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="obtainSMSContact" id="BPMNShape_obtainSMSContact">
        <omgdc:Bounds height="62.0" width="105.0" x="440.0" y="168.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
