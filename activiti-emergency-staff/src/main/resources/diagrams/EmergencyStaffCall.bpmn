      <bpmndi:BPMNShape bpmnElement="checkConfirmations" id="BPMNShape_checkConfirmations">
        <omgdc:Bounds height="65.0" width="105.0" x="810.0" y="165.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="allResponded" id="BPMNShape_allResponded">
        <omgdc:Bounds height="40.0" width="40.0" x="966.0" y="178.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="informAboutResult" id="BPMNShape_informAboutResult">
        <omgdc:Bounds height="61.0" width="105.0" x="1050.0" y="168.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endevent1" id="BPMNShape_endevent1">
